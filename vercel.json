{
    "version": 2,
    "builds": [
      {
        "src": "front/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "front/dist"
        }
      },
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "/front/dist/$1"
      }
    ]
  }
  